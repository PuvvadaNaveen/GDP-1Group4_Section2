<% include header %>

    
     
    <link rel ="stylesheet" type="text/css" href="/css/Rentallist.css"/> 
   
  
    <body>
      <div>
        <button type = "button" class="btn btn-success x" onclick="window.print()"> Print </button>
        </div> 
        <table id="rent">
          <tr>
            <th>Name</th>
            <th>Rented Item</th>
            <th>Rented date</th>
            <th>Return Date</th>
            <th>Rental Fee</th>
          </tr>
    <% for(let i = 0; i < list.length; i++) { %>
    <tr>
      <td><%= list[i].firstname %></td>
      <td><%= list[i].rental_item %></td>
      <td><%= list[i].StartDate %></td>
      <td><%= list[i].EndDate %></td>
      <td><%= list[i].fee %></td>
    </tr>
    <% } %>
  </table>
