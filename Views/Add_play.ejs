<body>    
        <br>         
       <h2>Add Plays</h2>
                      
<div class="container">
    <form method="POST" action="PlayController/play1">
  <div class="row">
    <div class="col-25">
      <label for="playname">Play Name</label>
    </div>
    <div class="col-75">
      <input type="text" id="D" name="playname" placeholder="Enter name.." required>
    </div>
  </div>                                           
  <div class="row">
    <div class="col-25">
      <label for="Play Start Date">Play Start Date</label>
     </div>
    <div class="col-75">
      <input type="date" id="D" placeholder="Enter Start Date" name="PlayStartDate" required>
    </div>
  </div>
  <div class="row">
     <div class="col-25">
        <label for="Play Due Date">Play End Date</label>
      </div>
      <div class="col-75">
        <input type="date" id="D" placeholder="Enter End Date" name="PlayEndDate"  required>
      </div>
  </div>
                                      <br>
  <div class="row">
    <input type="submit" value="Submit">
  </div>
</form>
  </div>
</body>